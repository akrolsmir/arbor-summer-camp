---
import Contact from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Contact - Arbor Summer Camp",
	description: "Reach out to Arbor Summer Camp with questions, suggestions, or to learn more about our events.",
	canonical: "https://arborsummercamp.com/contact/",
};
---

<BaseLayout seo={seo}>
	<Contact />
</BaseLayout>
