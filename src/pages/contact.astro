---
import Contact from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Contact",
	description: "Reach out to Arbor with questions, suggestions, or to learn more about our events.",
	canonical: "https://arborsummer.camp/contact/",
};
---

<BaseLayout seo={seo}>
	<Contact />
</BaseLayout>
